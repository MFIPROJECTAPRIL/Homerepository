		return si.insertBranch();
	}

	@RequestMapping("/district")
	public Branch saveDistrict(@RequestBody District district) {
		return null;
	}

	@RequestMapping("/role")
	public Branch saveRole(@RequestBody Role role) {
		return null;
	}

	@RequestMapping("/state")
	public Branch saveState(@RequestBody State state) {
		return null;
	}

	@RequestMapping("/status")
	public Branch saveStatus(@RequestBody Status status) {
		return null;
	}

	@RequestMapping("/taluka")
	public Branch saveTaluka(@RequestBody Taluka taluka) {
		return null;
	}

	@RequestMapping("/user")
	public Branch saveUser(@RequestBody User user) {
		return null;
	}

}
